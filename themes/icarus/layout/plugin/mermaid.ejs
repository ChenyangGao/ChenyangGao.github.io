<% if (head && get_config_from_obj(plugin, 'enable')) { %>
<style><%- plugin.cssSelector %> {background-color:white;}</style>
    <% if (get_config_from_obj(plugin, 'version')) { %>
<script src="https://unpkg.com/mermaid@<%= plugin['version'] %>/dist/mermaid.min.js"></script>
    <% } else { %>
<script src="https://unpkg.com/mermaid/dist/mermaid.min.js"></script>
    <% } %>
<script>
    if (window.mermaid) {
        mermaid.initialize(<%- JSON.stringify(plugin.config) %>, "<%- plugin.cssSelector %>");
    }
</script>
<% } %>