<% if (head && get_config_from_obj(plugin, 'enable')) { %>
<!-- require APlayer -->
    <% if (get_config_from_obj(plugin, 'aplyerVersion')) { %>
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/aplayer@<%= plugin.aplyerVersion %>/dist/APlayer.min.css">
<script src="https://cdn.jsdelivr.net/npm/aplayer@<%= plugin.aplyerVersion %>/dist/APlayer.min.js"></script>
    <% } else { %>
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/aplayer/dist/APlayer.min.css">
<script src="https://cdn.jsdelivr.net/npm/aplayer/dist/APlayer.min.js"></script>
    <% } %>
<!-- require MetingJS -->
<% if (get_config_from_obj(plugin, 'version')) { %>
<script src="https://cdn.jsdelivr.net/npm/meting@<%= plugin.version %>/dist/Meting.min.js"></script>
    <% } else { %>
<script src="https://cdn.jsdelivr.net/npm/meting/dist/Meting.min.js"></script>
    <% } %>
<!-- main music player -->
<%- get_config_from_obj(plugin, 'mainPlayer') %>
<% } %>