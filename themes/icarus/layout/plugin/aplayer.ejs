<% if (head && get_config_from_obj(plugin, 'enable')) { %>
    <% if (get_config_from_obj(plugin, 'version')) { %>
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/aplayer@<%= plugin.version %>/dist/APlayer.min.css">
<script src="https://cdn.jsdelivr.net/npm/aplayer@<%= plugin.version %>/dist/APlayer.min.js"></script>
    <% } else { %>
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/aplayer/dist/APlayer.min.css">
<script src="https://cdn.jsdelivr.net/npm/aplayer/dist/APlayer.min.js"></script>
    <% } %>
<div id="aplayer" class="music"></div>

<script>
(function () {
    new APlayer({
        container: document.querySelector("<%- plugin.cssSelector || '#aplayer' %>"),
        audio: JSON.parse('<%- JSON.stringify(site.data.musics) %>'),
        ...<%- JSON.stringify(plugin.config || {}) %>
    });
})();
</script>
<% } %>